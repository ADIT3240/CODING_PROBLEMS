#include <bits/stdc++.h>
using namespace std;

string cycleDetection(vector<vector<int>>& edges, int n, int m) {     // SIMPLE GIVEN FUNCTION 
    unordered_map<int, list<int>> adj;                                // MAKING ADJ MAP

    // Build adjacency list
    for (int i = 0; i < m; i++) {                                     // MAKING ADJ LIST
        int u = edges[i][0];
        int v = edges[i][1];

        adj[u].push_back(v);
        adj[v].push_back(u);
    }

    vector<bool> visited(n, false);                                   // SIMPLE VISITED ARRAY

    // BFS for each component
    for (int i = 0; i < n; i++) {                                     // FOR LOOP TO HANDLE DISCONNECTED COMPONENTS 
        if (!visited[i]) {
            unordered_map<int, int> parent;                           // ADDITIONAL DATA STRUCTURE -- PARENT NAAM KA 
            queue<int> q;                                             // QUEUE TOH BANTI HI HAI BFS MEIN                
 
            visited[i] = true;                                        // VISITED TRUE AS IN BFS
            q.push(i);                                                // SIMPLE PUSH IN BFS
            parent[i] = -1;                                           // ADDITIONALLY PARENT ASSIGN 

            while (!q.empty()) { 
                int top = q.front();                                  // SIMPLE QUEUE KI PROCESSING AS IN BFS 
                q.pop();

                for (auto neighbor : adj[top]) {                      // SIMPLE KI AGAR VISITED NAHI HAI TOH ....   
                    if (!visited[neighbor]) {
                        visited[neighbor] = true;
                        parent[neighbor] = top;                       // BSS PARENT KO BHI ASSIGN KARNA YAAD RAKHO
                        q.push(neighbor);
                    } 
                    // If visited and not parent â†’ cycle found        
                    else if (neighbor != parent[top]) {               // ADDITIONAL THING TO REMEMBER 
                        return "Yes";
                    }
                }
            }
        }
    }

    return "No";                                                      // JAHA FOR LOOP KHATAM HOTA HAI , WAHA FALSE STATEMENT DAALNI HAI 
}


// SAARA CODE IS SAME AS BFS ...
// BSS AAPKO EK PARENT NAAM KE DS KO TIME TO TIME UPDATE KARTA REHNA HAI
// else if (neighbor != parent[top]) {return "Yes";} BSS IK STATEMENT KO YAAD RAKHO AND EVERYTING IS SAME AS BFS .

